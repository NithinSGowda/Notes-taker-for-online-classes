{
    "manifest_version": 2,
    "name": "Classes - Meetings Listner",
    "version": "1.0.0",
    "description": "Class/Meetings Listener | Report generator - By Nithin S",
    "icons": {
      "128": "icon.png"
    },
    "browser_action": {
      "default_icon":"icon.png",
      "default_title": "Classes - Meetings Listner",
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["https://teams.microsoft.com/*","https://meet.google.com/*","https://*.zoom.us/*"],
      "js": ["jquery-3.5.1.js","contentscriptnithSummary.js"]
    }],
    "permissions": [
      "storage",
      "activeTab"
    ]
  }
  